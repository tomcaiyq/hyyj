
#selectMenu ul .moreOn{
    background-color: #fff !important;
    color: #5797dc !important;
  }
  .el-menu #selectMenu ul .is-active{
    background-color: #5599FF !important;
    color: black !important;
    font-weight: bold !important;
  }
  #selectMenu ul .more{
    line-height: 25px;
    background-color: #fff !important;
    color: #5797dc !important;
  }
  .el-menu-item {
    white-space: inherit;
  }
  .el-menu-item-group > ul {
    padding: 0;
    background-color: #fff !important;
    color: #5797dc !important;
    font-size: 16px !important;
  }
  #selectMenu .el-menu-item-group__title {
      padding: 7px 0 7px 20px;
      line-height: normal;
      font-size: 12px;
      color: #909399;
      display: none;
  }
  .el-menu {
      border-right: none;
      list-style: none;
      position: relative;
      margin: 0;
      padding-left: 0;
  }
  
