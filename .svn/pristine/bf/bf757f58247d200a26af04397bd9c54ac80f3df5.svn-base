html,
body,
#app {
  height: 100%;
  padding: 0;
  margin: 0;
  /* 控制横向上的最小宽度 */
  min-width: 747px;
  background-color: #fff;
  font-size: 16px;
}
a {
  text-decoration: none;
  color: #333;
}
ul li {
  list-style-type: none;
  padding: 0;
  margin: 0;
  cursor: default;
}
p{
  margin: 0;
}
/* 清除浮动 */
.clearfix::after {
  content: "";
  display: block;
  clear: both;
  height: 0px;
  line-height: 0px;
  visibility: hidden;
}
.clearfix {
  zoom: 1;
}
h1, h2, h3, h4, h5, h6, th { font-size:16px; font-weight:bold;color: #000000;margin: 0; }
p{font-size: 16px;}
.content{
  width: 1003px;
  margin:0 auto;
}
